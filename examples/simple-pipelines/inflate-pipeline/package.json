{
  "name": "inflate-pipeline",
  "description": "An example showcasing how to inflate archives.",
  "version": "0.3.0",
  "private": true,
  "scripts": {
    "build": "npx tsc",
    "build-pkg": "npx lerna run build --scope=inflate-pipeline --include-dependencies",
    "clean": "npx rimraf dist/ cdk.out/ node_modules/",
    "audit": "npm audit && npm run synth --silent | cfn_nag",
    "lint": "npx eslint .",
    "synth": "npx cdk synth",
    "deploy": "npx cdk deploy",
    "hotswap": "npx cdk deploy --hotswap",
    "destroy": "npx cdk destroy --all"
  },
  "author": {
    "name": "Amazon Web Services",
    "url": "https://aws.amazon.com"
  },
  "repository": {
    "type": "git",
    "url": "git://github.com/awslabs/project-lakechain"
  },
  "license": "Apache-2.0",
  "devDependencies": {
    "@types/node": "^20.8.10",
    "esbuild": "^0.20.0",
    "rimraf": "^5.0.5",
    "ts-node": "^10.9.2"
  },
  "dependencies": {
    "@project-lakechain/s3-event-trigger": "^0.3.0",
    "@project-lakechain/s3-storage-connector": "^0.3.0",
    "@project-lakechain/tar-inflate-processor": "^0.3.0",
    "@project-lakechain/zip-inflate-processor": "^0.3.0"
  },
  "peerDependencies": {
    "aws-cdk-lib": "^2.126.0",
    "constructs": "^10.0.0"
  }
}

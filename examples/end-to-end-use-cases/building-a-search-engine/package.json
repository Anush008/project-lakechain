{
  "name": "building-a-search-engine",
  "description": "End-to-end search engine multi-modal ingestion.",
  "version": "0.2.0",
  "private": true,
  "scripts": {
    "build": "npx tsc",
    "build-pkg": "npx lerna run build --scope=building-a-search-engine --include-dependencies",
    "clean": "npx rimraf dist/ cdk.out/ node_modules/ server/__pycache__",
    "audit": "npm audit && npm run synth --silent | cfn_nag",
    "lint": "npx eslint .",
    "synth": "npx cdk synth",
    "deploy": "npx cdk deploy",
    "hotswap": "npx cdk deploy --hotswap",
    "destroy": "npx cdk destroy --all"
  },
  "author": {
    "name": "Amazon Web Services",
    "url": "https://aws.amazon.com"
  },
  "repository": {
    "type": "git",
    "url": "git://github.com/awslabs/project-lakechain"
  },
  "license": "Apache-2.0",
  "devDependencies": {
    "@types/node": "^20.11.16",
    "esbuild": "^0.20.0",
    "ts-jest": "^29.1.2",
    "ts-node": "^10.9.2"
  },
  "dependencies": {
    "@project-lakechain/bedrock-embedding-processors": "^0.2.0",
    "@project-lakechain/clip-image-processor": "^0.2.0",
    "@project-lakechain/keybert-text-processor": "^0.2.0",
    "@project-lakechain/opensearch-domain": "^0.2.0",
    "@project-lakechain/opensearch-vector-storage-connector": "^0.2.0",
    "@project-lakechain/pandoc-text-converter": "^0.2.0",
    "@project-lakechain/pdf-text-converter": "^0.2.0",
